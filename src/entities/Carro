public class Carro{
    
    private Integer ano;
    private String marca;
    private String modelo;
    private String cor;
    
    public Carro(Integer ano,  String marca, String modelo, String cor){
    this.ano = ano;
    this.marca = marca;
    this.modelo = modelo;
    this.cor = cor;
    }
    
     public String exibirModelo(){
        return modelo;
    }
    public void  exibirAnoECor() {
        System.out.println(ano);
        System.out.println(cor);
    }
}
